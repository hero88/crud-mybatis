<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="DB2Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/coinmarket"
        userId="root"
        password="123456">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>
    
						<!-- create model at address package  -->		<!-- Name project -->
    <javaModelGenerator targetPackage="com.allxone.coinmarket.model" targetProject="mybatis-spring-boot-coinmarket"/>
     
												
    <sqlMapGenerator targetPackage="com.allxone.coinmarket.mapper.sql"  targetProject="mybatis-spring-boot-coinmarket/src/main/resources"/>
      
						<!-- create model at address package  -->							<!-- Name project -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.allxone.coinmarket.mapper"  targetProject="mybatis-spring-boot-coinmarket"/>
     

    <table tableName="holidays" domainObjectName="Holidays">
      
    </table>	
    <table tableName="insurance_type" domainObjectName="insuranceType">
      
    </table>


  </context>
</generatorConfiguration>